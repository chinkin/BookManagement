<mvc:View
    controllerName="BookManagement.controller.Landing"
	xmlns="sap.m"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true">
	<App class="myBookManagement">
        <pages>
            <Page title="{i18n>homePageTitle}">
                <headerContent>
					<Button
						icon="sap-icon://hello-world"
						press="onOpenDialog"/>
				</headerContent>
                <content>
                    <mvc:XMLView viewName="BookManagement.view.HelloPanel"/>
                    <mvc:XMLView viewName="BookManagement.view.InvoiceList"/>
                </content>
            </Page>
        </pages>
    </App>
</mvc:View>